<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" onMouseClicked="#statusShow" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ImageView fx:id="view" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <Button mnemonicParsing="false" onAction="#onPay" text="Заправка монеты" onMouseClicked="#statusShow" />
      <Button mnemonicParsing="false" onAction="#onStart" text="Запуск игры" />
      <Label fx:id="coinsCount" text="Label" />
   </children>

</VBox>
